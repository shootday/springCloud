<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.study.cn.spring_security.dao.IUserDao">

    <select id="loadUserByUsername" parameterType="java.lang.String" resultType="Users">
        select * from Users where username=#{username}
    </select>
</mapper>